{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@api/*": ["src/api/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@screens/*": ["src/screens/*"],
      "@store/*": ["src/store/*"],
      "@UI/*": ["src/UI/*"],
      "@types/*": ["src/types/*"],
      "@env": [".env"]
    },
    "types": [
      "@types/styled-components-react-native"
    ]
  },
}

